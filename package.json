{
  "name": "babylon-explorer",
  "version": "0.0.4",
  "description": "Babylon runtime (_r) explorer",
  "main": "index.js",
  "repository": "https://github.com/axeon-software/babylon-explorer",
  "scripts": {
    "build": "build --win",
    "ship": "build --win -p always",
    "start" : "electron ."
  },
  "build" : {
    "appId" : "babylon.runtime.explorer",
    "publish" : {
      "provider": "github",
      "token": "65e95268cbff01fe6e2bb0ed793171d08a73da83"
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": true,
      "perMachine": true
    },
    "fileAssociations" : {
      "ext" : ["gltf", "babylon"],
      "description" : "3D Real Time scenes"
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron-builder": "^20.10.0"
  },
  "dependencies": {
    "electron-updater": "^2.21.8"
  }
}
